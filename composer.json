{
    "name": "hitgrab/mailchimp-marketing-php",
    "version": "4.0.0",
    "description": "Mailchimp Marketing API for PHP 8.x",
    "homepage": "https://github.com/HitGrab/mailchimp-marketing-php",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mailchimp",
            "homepage": "https://github.com/mailchimp/mailchimp-marketing-php"
        }
    ],
    "require": {
        "php": ">=7.4|>=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "guzzlehttp/guzzle": "^6.4 || ^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": {
            "MailchimpMarketing\\" : "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": { "MailchimpMarketing\\" : "test/" }
    }
}
